<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "redu.dianqiuxiang_kehu1"
        version   = "1.0.0">
  <name>测试电话通讯录</name>

  <description>
    测试电话通讯录
  </description>
  <access origin="*" />
  <gap:plugin name="org.apache.cordova.battery-status" />
  <gap:plugin name="org.apache.cordova.camera" />
  <gap:plugin name="org.apache.cordova.media-capture" />
  <gap:plugin name="org.apache.cordova.console" />
  <gap:plugin name="org.apache.cordova.contacts" />
  <gap:plugin name="org.apache.cordova.device" />
  <gap:plugin name="org.apache.cordova.device-motion" />
  <gap:plugin name="org.apache.cordova.device-orientation" />
  <gap:plugin name="org.apache.cordova.dialogs" />
  <gap:plugin name="org.apache.cordova.file" />
  <gap:plugin name="org.apache.cordova.file-transfer" />
  <gap:plugin name="org.apache.cordova.geolocation" />
  <gap:plugin name="org.apache.cordova.globalization" />
  <gap:plugin name="org.apache.cordova.inappbrowser" />
  <gap:plugin name="org.apache.cordova.media" />
  <gap:plugin name="org.apache.cordova.network-information" />
  <gap:plugin name="org.apache.cordova.splashscreen" />
  <gap:plugin name="org.apache.cordova.vibration" />
  <gap:plugin name="com.phonegap.plugins.barcodescanner"/>


  <author href="http://phonegap.com" email="support@phonegap.com">
    点秋香
  </author>
  
  <feature name="Notification">
    <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
  </feature>
  
  <feature name="Vibration">
    <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
  </feature>

  <feature name="Contacts">
    <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
  </feature>
  
  <feature name="Contacts">
    <param name="ios-package" value="CDVContacts" />
  </feature>
  
  
  <preference name="orientation"      value="default" />
  <preference name="EnableViewportScale"      value="false" />
  <preference name="target-device"    value="handset" />
  <preference name="fullscreen"       value="true" />
  <preference name="webviewbounce"    value="false" />
  <preference name="splash-screen-duration" value="2000"/>
  <preference name="phonegap-version" value="3.5.0" />

  <gap:url-scheme name="com.acme.myscheme" role="None">
    <scheme>pgbr</scheme>
    <scheme>pgbw</scheme>
  </gap:url-scheme>

  <icon gap:platform="android" gap:qualifier="ldpi" src="res/icons/android/drawable-mdpi/ic_launcher.png" />
  <icon gap:platform="android" gap:qualifier="mdpi" src="res/icons/android/drawable-hdpi/ic_launcher.png" />
  <icon gap:platform="android" gap:qualifier="hdpi" src="res/icons/android/drawable-xhdpi/ic_launcher.png" />
  <icon gap:platform="android" gap:qualifier="xhdpi" src="res/icons/android/drawable-xxhdpi/ic_launcher.png" />
  <icon gap:platform="android" gap:qualifier="xxhdpi" src="res/icons/android/drawable-xxxhdpi/ic_launcher.png" />
  <icon src="res/icons/ios/icon_57.png"            gap:platform="ios"        width="57" height="57" />
  <icon src="res/icons/ios/icon_72.png"            gap:platform="ios"        width="72" height="72" />
  <icon src="res/icons/ios/icon_114.png"         gap:platform="ios"        width="114" height="114" />
  <icon src="res/icons/ios/icon_144.png"         gap:platform="ios"        width="144" height="144" />
  <icon src="res/icons/ios/icon_120.png"            gap:platform="ios"        width="120" height="120" />
  <icon src="res/icons/ios/icon_152.png"            gap:platform="ios"        width="152" height="152" />
  <icon src="res/icons/ios/icon_76.png"         gap:platform="ios"        width="76" height="76" />
  <gap:splash src="res/screen/android/480_800.png"  gap:platform="android" gap:density="ldpi" />
  <gap:splash src="res/screen/android/960_540.png"  gap:platform="android" gap:density="mdpi" />
  <gap:splash src="res/screen/android/1280_720.png"  gap:platform="android" gap:density="hdpi" />
  <gap:splash src="res/screen/android/1920_1080.png" gap:platform="android" gap:density="xhdpi" />
  <gap:splash src="res/screen/ios/320_480.png" gap:platform="ios" width="320" height="480" />
  <gap:splash src="res/screen/ios/640_960.png" gap:platform="ios" width="640" height="960" />
  <gap:splash src="res/screen/ios/640_1136.png" gap:platform="ios" height="1136"  width="640" />
  <!-- content src="http://www.w3.org/2010/05/video/mediaevents.html" / -->
</widget>
